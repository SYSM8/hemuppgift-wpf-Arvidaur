<Window x:Class="Hemuppgift_WPFtemplate.WindowDockPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="WindowDockPanel" Height="450" Width="800">
    <DockPanel>

        <Button x:Name="BtnTop" Content="Top" DockPanel.Dock="Top" HorizontalAlignment="Center" Width="80" Height="30" Margin="0,5"/>
        <Button x:Name="BtnRight" Content="Right" DockPanel.Dock="Right" VerticalAlignment="Center" Width="80" Height="30" Margin="5,0"/>
        <Button x:Name="BtnBottom" Content="Bottom" DockPanel.Dock="Bottom" HorizontalAlignment="Center" Width="80" Height="30" Margin="0,5"/>
        <Button x:Name="BtnLeft" Content="Left" DockPanel.Dock="Left" VerticalAlignment="Center" Width="80" Height="30" Margin="5,0"/>
        <Button x:Name="BtnCenter" Content="Center" VerticalAlignment="Center" HorizontalAlignment="Center" Width="80" Height="30" Click="BtnCenter_Click"/>

        <Image x:Name="ImgTrophy"/>
    </DockPanel>
</Window>
